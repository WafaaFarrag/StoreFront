<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<document type="com.apple.InterfaceBuilder3.CocoaTouch.Storyboard.XIB" version="3.0" toolsVersion="18122" targetRuntime="iOS.CocoaTouch" propertyAccessControl="none" useAutolayout="YES" launchScreen="YES" useTraitCollections="YES">
    <scenes>
        <!--Profile View Controller-->
        <scene sceneID="ProfileScene">
            <objects>
                <viewController id="ProfileVC" customClass="ProfileViewController" sceneMemberID="viewController">
                    <view key="view" contentMode="scaleToFill" id="ProfileView">
                        <subviews>
                            <imageView id="profileImage" contentMode="scaleAspectFill" clipsSubviews="YES"/>
                            <label id="nameLabel" text="Ahmed Mohammed"/>
                            <label id="usernameLabel" text="@Ahmedmohamed1995"/>
                            <stackView id="statsStack" axis="horizontal" distribution="fillEqually" alignment="center">
                                <subviews>
                                    <button id="followingButton" title="5 Following"/>
                                    <button id="followersButton" title="20 Followers"/>
                                </subviews>
                            </stackView>
                            <segmentedControl id="tabsSegmentedControl" segmentedControlStyle="plain">
                                <segments>
                                    <segment title="Products"/>
                                    <segment title="Reviews"/>
                                    <segment title="Followers"/>
                                </segments>
                            </segmentedControl>
                            <searchBar id="searchBar" placeholder="Search"/>
                            <collectionView id="productsCollectionView">
                                <collectionViewFlowLayout key="collectionViewLayout" minimumLineSpacing="10" minimumInteritemSpacing="10" scrollDirection="vertical"/>
                            </collectionView>
                            <stackView id="adsStack" axis="vertical" distribution="fillEqually" spacing="10">
                                <subviews>
                                    <imageView id="adBanner1"/>
                                    <imageView id="adBanner2"/>
                                </subviews>
                            </stackView>
                            <scrollView id="tagsScroll" showsHorizontalScrollIndicator="NO">
                                <subviews>
                                    <stackView id="tagsStack" axis="horizontal" spacing="8">
                                        <subviews>
                                            <button title="All"/>
                                            <button title="Accessories"/>
                                            <button title="Cars And Motorcycle"/>
                                            <button title="Sunglasses"/>
                                            <button title="Shoes"/>
                                            <button title="Makeup"/>
                                        </subviews>
                                    </stackView>
                                </subviews>
                            </scrollView>
                        </subviews>
                        <constraints/>
                    </view>
                </viewController>
            </objects>
        </scene>
    </scenes>
</document>
